<?xml version="1.0"?>
<block>
  <name>LoRa Hamming Encode</name>
  <key>lora2_lora_hamming_encode</key>
  <category>[lora2]</category>
  <import>import lora2</import>
  <make>lora2.lora_hamming_encode($CR, $len_tag_key)</make>

  <param>
    <name>CR</name>
    <key>CR</key>
    <type>int</type>
  </param>

  <param>
    <name>Length Tag Key</name>
    <key>len_tag_key</key>
    <type>string</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
