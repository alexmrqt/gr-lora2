<?xml version="1.0"?>
<block>
  <name>LoRa Preamble Detect</name>
  <key>lora2_lora_preamble_detect</key>
  <category>[lora2]</category>
  <import>import lora2</import>
  <make>lora2.lora_preamble_detect($SF, $preamble_len, thres=$thres)</make>
  <param>
    <name>SF</name>
    <key>SF</key>
    <type>int</type>
  </param>

  <param>
    <name>Threshold</name>
    <key>thres</key>
    <value>1e-4</value>
    <type>float</type>
  </param>

  <param>
    <name>Preamble length [6-65535]</name>
    <key>preamble_len</key>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
