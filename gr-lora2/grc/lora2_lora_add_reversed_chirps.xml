  <param>
    <name>SF</name>
	<key>SF</key>
    <type>int</type>
  </param>
  <param>
    <name>Interpolation</name>
	<key>interp</key>
    <type>int</type>
  </param>
  <param>
    <name>Length Tag Key</name>
	<key>len_tag_key</key>
    <type>string</type>
  </param>
  <param>
    <name>Preamble End Tag Key</name>
	<key>pre_end_tag_key</key>
    <type>string</type>
  </param>
  <param>
    <name>Rev Chirps End Tag Key</name>
	<key>rev_chirps_end_tag_key</key>
    <type>string</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
